<merge xmlns:android="http://schemas.android.com/apk/res/android"
       xmlns:app="http://schemas.android.com/apk/res-auto"
       xmlns:tools="http://schemas.android.com/tools">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/table" style="@style/SunsetTimeLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="start"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <!-- Header: date -->
        <TextView
            android:id="@+id/text_date" style="@style/TableHeadDateView"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:focusable="true" android:clickable="true"
            android:nextFocusLeft="@+id/header_time_sunset" android:nextFocusRight="@+id/header_time_sunrise"
            android:background="?attr/selectableItemBackground"
            tools:text="Today\nApril 1, 2016"

            app:layout_constraintHorizontal_chainStyle="spread"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/header_gutter0"


            app:layout_constraintTop_toTopOf="parent"
             />

        <!-- Header: gutter0 -->
        <LinearLayout
            android:id="@+id/header_gutter0"
            android:layout_width="0dp" android:layout_height="wrap_content"
            app:layout_constraintHorizontal_weight="@integer/table_weight0"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/text_date"
            app:layout_constraintStart_toEndOf="@+id/text_date"
            app:layout_constraintEnd_toStartOf="@+id/header_time_sunrise"
            android:orientation="vertical">
        </LinearLayout>

        <View android:id="@+id/header_column" android:elevation="0dp"
            android:layout_width="0dp" android:layout_height="2dp"
            android:background="@color/transparent" android:visibility="invisible"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/text_time_sunrise_actual" app:layout_constraintBottom_toBottomOf="parent" />

        <!-- Header: sunrise -->
        <LinearLayout
            android:id="@+id/header_time_sunrise"
            android:background="?attr/selectableItemBackground"
            android:focusable="true" android:clickable="true"
            android:nextFocusLeft="@+id/text_date"
            android:nextFocusRight="@+id/header_time_sunset"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/table_rise_leftMargin" android:layout_marginStart="@dimen/table_rise_leftMargin"
            android:layout_marginTop="8dp"
            android:minWidth="46dp" android:minHeight="16dp"

            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/text_date"
            app:layout_constraintStart_toEndOf="@+id/header_gutter0"
            app:layout_constraintEnd_toStartOf="@+id/header_time_sunset"
            android:orientation="vertical" android:gravity="bottom">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end|right"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/icon_time_sunrise"
                    style="@style/TableHeadImageView"
                    android:layout_width="@dimen/table_heading_iconsize"
                    android:background="?attr/sunriseIconSmall"
                    android:layout_gravity="bottom|center_horizontal"
                    android:layout_margin="0dp"
                    tools:ignore="contentDescription"/>

                <TextView android:id="@+id/label_time_sunrise" style="@style/TableHeadTextView"
                    android:text="@string/sunrise_short" android:textColor="?attr/table_risingColor"
                    android:maxWidth="@dimen/table_heading_label_maxdim" />

            </LinearLayout>

        </LinearLayout>

        <!-- Header: sunset -->
        <LinearLayout
            android:id="@+id/header_time_sunset"
            android:background="?attr/selectableItemBackground"
            android:focusable="true" android:clickable="true"
            android:nextFocusLeft="@+id/header_time_sunrise" android:nextFocusRight="@+id/text_date"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/table_set_leftMargin" android:layout_marginStart="@dimen/table_set_leftMargin"
            android:layout_marginTop="8dp"
            android:minWidth="@dimen/table_cell_minwidth" android:minHeight="16dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/text_date"
            app:layout_constraintStart_toEndOf="@+id/header_time_sunrise"
            app:layout_constraintEnd_toStartOf="@+id/header_gutter1"
            android:orientation="vertical" android:gravity="bottom">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end|right"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/icon_time_sunset"
                    style="@style/TableHeadImageView"
                    android:layout_width="@dimen/table_heading_iconsize"
                    android:background="?attr/sunsetIconSmall"
                    android:layout_gravity="bottom|center_horizontal"
                    android:layout_margin="0dp"
                    tools:ignore="contentDescription"/>

                <TextView
                    android:id="@+id/label_time_sunset"
                    style="@style/TableHeadTextView"
                    android:maxWidth="@dimen/table_heading_label_maxdim"
                    android:text="@string/sunset_short"
                    android:textColor="?attr/table_settingColor"/>

            </LinearLayout>

        </LinearLayout>

        <!-- Header: gutter1 -->
        <LinearLayout
            android:id="@+id/header_gutter1"
            android:layout_width="0dp" android:layout_height="wrap_content"
            app:layout_constraintHorizontal_weight="@integer/table_weight1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/text_date"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/header_time_sunset"
            android:orientation="vertical">
        </LinearLayout>

        <!-- Bucket 1 -->
        <LinearLayout android:id="@+id/bucket1_labels"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_date"
            android:layout_marginBottom="@dimen/table_cell_spacing"
            android:layout_marginTop="@dimen/table_cell_spacing"
            android:layout_marginLeft="2dp" android:layout_marginRight="2dp"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:layout_marginTop="0dp" android:layout_marginBottom="0dp" android:layout_marginLeft="0dp" android:layout_marginRight="0dp" android:padding="0dp"
                android:text="Custom 1" tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout android:id="@+id/bucket1_rising"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/bucket1_labels"
            app:layout_constraintBottom_toBottomOf="@+id/bucket1_labels"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunrise"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textColor="?attr/sunriseColor"
                android:text="11:00 AM" tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout android:id="@+id/bucket1_setting"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/bucket1_labels"
            app:layout_constraintBottom_toBottomOf="@+id/bucket1_labels"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunset"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textColor="?attr/sunsetColor" android:text="1:00 PM" tools:ignore="HardcodedText" />
        </LinearLayout>

        <!-- Astronomical -->
        <TextView
            android:id="@+id/text_time_label_astro"
            style="@style/SunsetTimeTextView"
            android:layout_marginTop="@dimen/table_cell_spacing"
            android:layout_marginBottom="@dimen/table_cell_spacing"
            android:text="@string/timeMode_astronomical_short"
            app:layout_constraintTop_toBottomOf="@+id/bucket1_labels"

            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/text_time_sunrise_astro"
            style="@style/SunsetTimeTextView"
            android:layout_marginEnd="@dimen/table_rise_rightMargin"
            android:layout_marginRight="@dimen/table_rise_rightMargin"
            android:textColor="?attr/table_risingColor"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_time_label_astro"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunrise"
            tools:text="00:00 AM"/>

        <TextView
            android:id="@+id/text_time_sunset_astro"
            style="@style/SunsetTimeTextView"
            android:layout_marginEnd="@dimen/table_set_rightMargin"
            android:layout_marginRight="@dimen/table_set_rightMargin"
            android:textColor="?attr/table_settingColor"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_time_label_astro"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunset"
            tools:text="00:00 PM"/>

        <!-- Bucket 2 -->
        <LinearLayout android:id="@+id/bucket2_labels"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_time_label_astro"
            android:layout_marginBottom="@dimen/table_cell_spacing"
            android:layout_marginTop="@dimen/table_cell_spacing"
            android:layout_marginLeft="2dp" android:layout_marginRight="2dp"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:layout_marginTop="0dp" android:layout_marginBottom="0dp" android:layout_marginLeft="0dp" android:layout_marginRight="0dp" android:padding="0dp"
                android:text="Custom 2" tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout android:id="@+id/bucket2_rising"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/bucket2_labels"
            app:layout_constraintBottom_toBottomOf="@+id/bucket2_labels"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunrise"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textColor="?attr/sunriseColor"
                android:text="11:00 AM" tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout android:id="@+id/bucket2_setting"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/bucket2_labels"
            app:layout_constraintBottom_toBottomOf="@+id/bucket2_labels"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunset"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textColor="?attr/sunsetColor" android:text="1:00 PM" tools:ignore="HardcodedText" />
        </LinearLayout>

        <!-- Nautical -->
        <TextView
            android:id="@+id/text_time_label_nautical"
            style="@style/SunsetTimeTextView"
            android:layout_marginBottom="@dimen/table_cell_spacing"
            android:layout_marginTop="@dimen/table_cell_spacing"
            android:text="@string/timeMode_nautical_short"
            app:layout_constraintTop_toBottomOf="@+id/bucket2_labels"

            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/text_time_sunrise_nautical"
            style="@style/SunsetTimeTextView"
            android:layout_marginEnd="@dimen/table_rise_rightMargin"
            android:layout_marginRight="@dimen/table_rise_rightMargin"
            android:textColor="?attr/table_risingColor"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_time_label_nautical"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunrise"
            tools:text="00:00 AM"/>

        <TextView
            android:id="@+id/text_time_sunset_nautical"
            style="@style/SunsetTimeTextView"
            android:layout_marginEnd="@dimen/table_set_rightMargin"
            android:layout_marginRight="@dimen/table_set_rightMargin"
            android:textColor="?attr/table_settingColor"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_time_label_nautical"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunset"
            tools:text="00:00 PM"/>

        <!-- Bucket 3 -->
        <LinearLayout android:id="@+id/bucket3_labels"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_time_label_nautical"
            android:layout_marginBottom="@dimen/table_cell_spacing"
            android:layout_marginTop="@dimen/table_cell_spacing"
            android:layout_marginLeft="2dp" android:layout_marginRight="2dp"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:layout_marginTop="0dp" android:layout_marginBottom="0dp" android:layout_marginLeft="0dp" android:layout_marginRight="0dp" android:padding="0dp"
                android:text="Custom 3" tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout android:id="@+id/bucket3_rising"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/bucket3_labels"
            app:layout_constraintBottom_toBottomOf="@+id/bucket3_labels"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunrise"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textColor="?attr/sunriseColor"
                android:text="11:00 AM" tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout android:id="@+id/bucket3_setting"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/bucket3_labels"
            app:layout_constraintBottom_toBottomOf="@+id/bucket3_labels"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunset"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textColor="?attr/sunsetColor" android:text="1:00 PM" tools:ignore="HardcodedText" />
        </LinearLayout>

        <!--Blue Hour (8 deg) -->
        <TextView
            android:id="@+id/text_time_label_blue8"
            style="@style/SunsetTimeTextView"
            android:layout_marginBottom="@dimen/table_cell_spacing"
            android:layout_marginTop="@dimen/table_cell_spacing"
            android:text="@string/timeMode_blue8_short"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bucket3_labels"
             />

        <TextView
            android:id="@+id/text_time_blue8_morning"
            style="@style/SunsetTimeTextView"
            android:layout_marginEnd="@dimen/table_set_rightMargin"
            android:layout_marginRight="@dimen/table_set_rightMargin"
            android:textColor="?attr/table_risingColor"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_time_label_blue8"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunrise"
            tools:text="00:00 PM"/>

        <TextView
            android:id="@+id/text_time_blue8_evening"
            style="@style/SunsetTimeTextView"
            android:layout_marginEnd="@dimen/table_set_rightMargin"
            android:layout_marginRight="@dimen/table_set_rightMargin"
            android:textColor="?attr/table_settingColor"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_time_label_blue8"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunset"
            tools:text="00:00 PM" />

        <!-- Bucket 4 -->
        <LinearLayout android:id="@+id/bucket4_labels"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_time_label_blue8"
            android:layout_marginBottom="@dimen/table_cell_spacing"
            android:layout_marginTop="@dimen/table_cell_spacing"
            android:layout_marginLeft="2dp" android:layout_marginRight="2dp"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:layout_marginTop="0dp" android:layout_marginBottom="0dp" android:layout_marginLeft="0dp" android:layout_marginRight="0dp" android:padding="0dp"
                android:text="Custom 4" tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout android:id="@+id/bucket4_rising"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/bucket4_labels"
            app:layout_constraintBottom_toBottomOf="@+id/bucket4_labels"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunrise"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textColor="?attr/sunriseColor"
                android:text="11:00 AM" tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout android:id="@+id/bucket4_setting"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/bucket4_labels"
            app:layout_constraintBottom_toBottomOf="@+id/bucket4_labels"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunset"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textColor="?attr/sunsetColor" android:text="1:00 PM" tools:ignore="HardcodedText" />
        </LinearLayout>

        <!-- Civil -->
        <TextView
            android:id="@+id/text_time_label_civil"
            style="@style/SunsetTimeTextView"
            android:layout_marginBottom="@dimen/table_cell_spacing"
            android:layout_marginTop="@dimen/table_cell_spacing"
            android:text="@string/timeMode_civil_short"
            app:layout_constraintTop_toBottomOf="@+id/bucket4_labels"

            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/text_time_sunrise_civil"
            style="@style/SunsetTimeTextView"
            android:layout_marginEnd="@dimen/table_rise_rightMargin"
            android:layout_marginRight="@dimen/table_rise_rightMargin"
            android:textColor="?attr/table_risingColor"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_time_label_civil"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunrise"
            tools:text="00:00 AM"/>

        <TextView
            android:id="@+id/text_time_sunset_civil"
            style="@style/SunsetTimeTextView"
            android:layout_marginEnd="@dimen/table_set_rightMargin"
            android:layout_marginRight="@dimen/table_set_rightMargin"
            android:textColor="?attr/table_settingColor"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_time_label_civil"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunset"
            tools:text="00:00 PM"/>

        <!-- Bucket 5 -->
        <LinearLayout android:id="@+id/bucket5_labels"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_time_label_civil"
            android:layout_marginBottom="@dimen/table_cell_spacing"
            android:layout_marginTop="@dimen/table_cell_spacing"
            android:layout_marginLeft="2dp" android:layout_marginRight="2dp"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:layout_marginTop="0dp" android:layout_marginBottom="0dp" android:layout_marginLeft="0dp" android:layout_marginRight="0dp" android:padding="0dp"
                android:text="Custom 5" tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout android:id="@+id/bucket5_rising"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/bucket5_labels"
            app:layout_constraintBottom_toBottomOf="@+id/bucket5_labels"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunrise"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textColor="?attr/sunriseColor"
                android:text="11:00 AM" tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout android:id="@+id/bucket5_setting"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/bucket5_labels"
            app:layout_constraintBottom_toBottomOf="@+id/bucket5_labels"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunset"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textColor="?attr/sunsetColor" android:text="1:00 PM" tools:ignore="HardcodedText" />
        </LinearLayout>

        <!--Blue Hour (4 deg) -->
        <TextView
            android:id="@+id/text_time_label_blue4"
            style="@style/SunsetTimeTextView"
            android:layout_marginBottom="@dimen/table_cell_spacing"
            android:layout_marginTop="@dimen/table_cell_spacing"
            android:text="@string/timeMode_blue4_short"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bucket5_labels"
            />

        <TextView
            android:id="@+id/text_time_blue4_morning"
            style="@style/SunsetTimeTextView"
            android:layout_marginEnd="@dimen/table_set_rightMargin"
            android:layout_marginRight="@dimen/table_set_rightMargin"
            android:textColor="?attr/table_settingColor"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_time_label_blue4"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunrise"
            tools:text="00:00 PM"/>

        <TextView
            android:id="@+id/text_time_blue4_evening"
            style="@style/SunsetTimeTextView"
            android:layout_marginEnd="@dimen/table_set_rightMargin"
            android:layout_marginRight="@dimen/table_set_rightMargin"
            android:textColor="?attr/table_risingColor"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_time_label_blue4"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunset"
            tools:text="00:00 PM" />

        <!-- Bucket 6 -->
        <LinearLayout android:id="@+id/bucket6_labels"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_time_label_blue4"
            android:layout_marginBottom="@dimen/table_cell_spacing"
            android:layout_marginTop="@dimen/table_cell_spacing"
            android:layout_marginLeft="2dp" android:layout_marginRight="2dp"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:layout_marginTop="0dp" android:layout_marginBottom="0dp" android:layout_marginLeft="0dp" android:layout_marginRight="0dp" android:padding="0dp"
                android:text="Custom 6" tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout android:id="@+id/bucket6_rising"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/bucket6_labels"
            app:layout_constraintBottom_toBottomOf="@+id/bucket6_labels"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunrise"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textColor="?attr/sunriseColor"
                android:text="11:00 AM" tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout android:id="@+id/bucket6_setting"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/bucket6_labels"
            app:layout_constraintBottom_toBottomOf="@+id/bucket6_labels"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunset"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textColor="?attr/sunsetColor" android:text="1:00 PM" tools:ignore="HardcodedText" />
        </LinearLayout>

        <!-- Official -->
        <TextView
            android:id="@+id/text_time_label_official"
            style="@style/SunsetTimeTextView"
            android:layout_marginBottom="@dimen/table_cell_spacing"
            android:layout_marginTop="@dimen/table_cell_spacing"
            android:text="@string/timeMode_official_short"
            app:layout_constraintTop_toBottomOf="@+id/bucket6_labels"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/text_time_sunrise_actual"
            style="@style/SunsetTimeTextView"
            android:layout_marginEnd="@dimen/table_rise_rightMargin"
            android:layout_marginRight="@dimen/table_rise_rightMargin"
            android:textColor="?attr/table_risingColor"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_time_label_official"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunrise"
            tools:text="00:00 AM"/>

        <TextView
            android:id="@+id/text_time_sunset_actual"
            style="@style/SunsetTimeTextView"
            android:layout_marginEnd="@dimen/table_set_rightMargin"
            android:layout_marginRight="@dimen/table_set_rightMargin"
            android:textColor="?attr/table_settingColor"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_time_label_official"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunset"
            tools:text="00:00 PM"/>

        <!-- horizontal rule -->
        <View
            android:id="@+id/table_rule"
            style="@style/SunsetTimeHorizontalRule"
            android:layout_marginTop="@dimen/table_cell_spacing"
            android:padding="8dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunset"
            app:layout_constraintTop_toBottomOf="@+id/text_time_label_official"
             />

        <View android:id="@+id/table_rule1"
            android:layout_width="0dp" android:layout_height="@dimen/table_cell_spacing_half"
            android:layout_margin="0dp" android:padding="0dp"
            app:layout_constraintStart_toStartOf="@+id/table_rule"
            app:layout_constraintEnd_toEndOf="@+id/table_rule"
            app:layout_constraintTop_toBottomOf="@+id/table_rule" />

        <!-- Bucket 7 -->
        <LinearLayout android:id="@+id/bucket7_labels"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/table_rule1"
            android:layout_marginLeft="2dp" android:layout_marginRight="2dp"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:layout_marginTop="0dp" android:layout_marginBottom="0dp" android:layout_marginLeft="0dp" android:layout_marginRight="0dp" android:padding="0dp"
                android:text="Custom 7" tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout android:id="@+id/bucket7_rising"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/bucket7_labels"
            app:layout_constraintBottom_toBottomOf="@+id/bucket7_labels"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunrise"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textColor="?attr/sunriseColor"
                android:text="11:00 AM" tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout android:id="@+id/bucket7_setting"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/bucket7_labels"
            app:layout_constraintBottom_toBottomOf="@+id/bucket7_labels"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunset"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textColor="?attr/sunsetColor" android:text="1:00 PM" tools:ignore="HardcodedText" />
        </LinearLayout>

        <!-- Golden Hour -->
        <TextView
            android:id="@+id/text_time_label_golden"
            style="@style/SunsetTimeTextView"
            android:layout_marginBottom="@dimen/table_cell_spacing"
            android:layout_marginTop="@dimen/table_cell_spacing"
            android:text="@string/timeMode_golden_short"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bucket7_labels" />

        <TextView
            android:id="@+id/text_time_golden_morning"
            style="@style/SunsetTimeTextView"
            android:layout_marginEnd="@dimen/table_set_rightMargin"
            android:layout_marginRight="@dimen/table_set_rightMargin"
            android:textColor="?attr/table_settingColor"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_time_label_golden"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunrise"
            tools:text="00:00 PM"/>

        <TextView
            android:id="@+id/text_time_golden_evening"
            style="@style/SunsetTimeTextView"
            android:layout_marginEnd="@dimen/table_set_rightMargin"
            android:layout_marginRight="@dimen/table_set_rightMargin"
            android:textColor="?attr/table_risingColor"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_time_label_golden"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunset"
            tools:text="00:00 PM" />

        <!-- Bucket 8 -->
        <LinearLayout android:id="@+id/bucket8_labels"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_time_label_golden"
            android:layout_marginTop="@dimen/table_cell_spacing"
            android:layout_marginBottom="@dimen/table_cell_spacing"
            android:layout_marginLeft="2dp" android:layout_marginRight="2dp"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:layout_marginTop="0dp" android:layout_marginBottom="0dp" android:layout_marginLeft="0dp" android:layout_marginRight="0dp" android:padding="0dp"
                android:text="Custom 8" tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout android:id="@+id/bucket8_rising"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/bucket8_labels"
            app:layout_constraintBottom_toBottomOf="@+id/bucket8_labels"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunrise"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textColor="?attr/sunriseColor"
                android:text="11:00 AM" tools:ignore="HardcodedText" />
        </LinearLayout>
        <LinearLayout android:id="@+id/bucket8_setting"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@+id/bucket8_labels"
            app:layout_constraintBottom_toBottomOf="@+id/bucket8_labels"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunset"
            android:orientation="vertical">
            <TextView style="@style/SunsetTimeTextView"
                android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:textColor="?attr/sunsetColor" android:text="1:00 PM" tools:ignore="HardcodedText" />
        </LinearLayout>

        <!-- Solar Noon -->
        <TextView
            android:id="@+id/text_time_label_noon"
            style="@style/SunsetTimeTextView"
            android:layout_marginTop="0dp" android:paddingTop="@dimen/table_cell_spacing_half"
            android:layout_marginBottom="0dp" android:paddingBottom="@dimen/table_cell_spacing_half"
            android:text="@string/timeMode_noon"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bucket8_labels"
             />

        <TextView
            android:id="@+id/text_position_noon"
            style="@style/SunsetTimeTextView"
            android:layout_marginEnd="@dimen/table_set_rightMargin"
            android:layout_marginRight="@dimen/table_set_rightMargin"
            android:textColor="?attr/table_noonColor"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_time_label_noon"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunrise"
            tools:text="33°∠"/>

        <TextView
            android:id="@+id/text_time_noon"
            style="@style/SunsetTimeTextView"
            android:layout_marginEnd="@dimen/table_set_rightMargin"
            android:layout_marginRight="@dimen/table_set_rightMargin"
            android:textColor="?attr/table_noonColor"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_time_label_noon"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunset"
            tools:text="00:00 PM"/>

        <TextView
            android:id="@+id/text_time_label_midnight"
            style="@style/SunsetTimeTextView"
            android:layout_marginTop="0dp" android:paddingTop="@dimen/table_cell_spacing_half"
            android:layout_marginBottom="0dp" android:paddingBottom="@dimen/table_cell_spacing_half"
            android:text="@string/timeMode_midnight"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_position_noon"
            />

        <TextView
            android:id="@+id/text_time_midnight"
            style="@style/SunsetTimeTextView"
            android:layout_marginEnd="@dimen/table_set_rightMargin"
            android:layout_marginRight="@dimen/table_set_rightMargin"
            android:textColor="?attr/table_nightColor"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_time_label_midnight"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunset"
            tools:text="00:00 PM"/>

        <View
            android:id="@+id/noon_clickArea" android:orientation="horizontal"
            android:layout_width="0dp" android:layout_height="0dp" android:elevation="2dp"
            android:background="?attr/selectableItemBackground" android:clickable="true" android:focusable="true"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/text_time_label_noon"
            app:layout_constraintBottom_toBottomOf="@+id/text_time_label_midnight" />

        <!-- Moon -->
        <TextView
            android:id="@+id/text_time_label_moon"
            style="@style/SunsetTimeTextView"
            android:layout_marginTop="0dp" android:paddingTop="@dimen/table_cell_spacing_half"
            android:layout_marginBottom="0dp" android:paddingBottom="0dp"
            android:text="@string/timeMode_moon"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_time_label_midnight" />

        <com.forrestguice.suntimeswidget.moon.MoonPhaseView
            android:id="@+id/moonphase_view" app:illuminationRange="true"
            android:layout_width="0dp" android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/moonriseset_view"
            app:layout_constraintTop_toBottomOf="@+id/text_time_label_moon" />

        <com.forrestguice.suntimeswidget.moon.MoonRiseSetView
            android:id="@+id/moonriseset_view"
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/text_time_label_moon"
            app:layout_constraintBottom_toBottomOf="@+id/moonphase_view"
            app:layout_constraintEnd_toEndOf="@+id/header_time_sunset" />

        <LinearLayout
            android:id="@+id/moonphase_clickArea" android:orientation="horizontal"
            android:layout_width="0dp" android:layout_height="0dp" android:elevation="2dp"
            android:background="?attr/selectableItemBackground" android:clickable="true" android:focusable="true"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/text_time_label_moon"
            app:layout_constraintBottom_toBottomOf="@+id/moonphase_view" />

    </android.support.constraint.ConstraintLayout>

</merge>