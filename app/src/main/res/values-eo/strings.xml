    <!-- Dialog: Help -->
    <string name="help_dialog_title">Helpo</string>
    <string name="help_appearance_title"><![CDATA[
        La jenaj mallongigoj estas disponeblaj: <br />
                * <b>%loc</b> por etikedo <br />
                * <b>%lat</b> por latitudo <br />
                * <b>%lon</b> por longitudo <br/>
                * <b>%m</b> por temp-reĝimo (mallonge) (ekz. "Civila") <br/>
                * <b>%M</b> por temp-reĝimo (longe) (ekz. "Civila krepusko") <br/>
