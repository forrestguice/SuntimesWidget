<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true" android:state_enabled="true"
        android:drawable="@drawable/chip_contrast_enabled_focused_light" />

    <item android:state_focused="true" android:state_enabled="false"
        android:drawable="@drawable/chip_contrast_disabled_focused_light" />

    <item android:state_pressed="true" android:state_enabled="true"
        android:drawable="@drawable/chip_contrast_pressed_light" />

    <item android:state_pressed="true" android:state_enabled="false"
        android:drawable="@drawable/chip_contrast_disabled_light" />

    <item android:state_enabled="false"
        android:drawable="@drawable/chip_contrast_disabled_light" />

    <item android:drawable="@drawable/chip_contrast_enabled_light" />
</selector>