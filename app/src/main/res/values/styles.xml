    <!-- view styles -->

    <!-- main activity card items -->
    <style name="SunsetTimeCard">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">?attr/timeCardBackground</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

    <!-- The outermost frame around the widget -->
    <style name="SunsetTimeWidgetFrame">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">@dimen/widget_margin</item>
    </style>
    <style name="SunsetTimeWidgetFrame1" parent="SunsetTimeWidgetFrame">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <!-- The inner frame around the widget -->
    <style name="SunsetTimeWidgetBase">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="SunsetTimeWidget" parent="SunsetTimeWidgetBase">
        <item name="android:background">@drawable/bg_dark</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingLeft">@dimen/widget_padding_left</item>
        <item name="android:paddingRight">@dimen/widget_padding_right</item>
        <item name="android:paddingTop">@dimen/widget_padding_top</item>
        <item name="android:paddingBottom">@dimen/widget_padding_bottom</item>
    </style>
    <style name="SunsetTimeWidget_dark" parent="SunsetTimeWidget">
        <item name="android:background">@null</item>
    </style>

    <style name="SunsetTimeWidget_float" parent="SunsetTimeWidget">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="SunsetTimeWidget_float_1" parent="SunsetTimeWidget_float">
        <item name="android:layout_gravity">top|left|start</item>
    </style>
    <style name="SunsetTimeWidget_float_2" parent="SunsetTimeWidget_float">
        <item name="android:layout_gravity">top|center_horizontal</item>
    </style>
    <style name="SunsetTimeWidget_float_3" parent="SunsetTimeWidget_float">
        <item name="android:layout_gravity">top|right|end</item>
    </style>
    <style name="SunsetTimeWidget_float_4" parent="SunsetTimeWidget_float">
        <item name="android:layout_gravity">center_vertical|left|start</item>
    </style>
    <style name="SunsetTimeWidget_float_5" parent="SunsetTimeWidget_float">
        <item name="android:layout_gravity">center</item>
    </style>
    <style name="SunsetTimeWidget_float_6" parent="SunsetTimeWidget_float">
        <item name="android:layout_gravity">center_vertical|right|end</item>
    </style>
    <style name="SunsetTimeWidget_float_7" parent="SunsetTimeWidget_float">
        <item name="android:layout_gravity">bottom|left|start</item>
    </style>
    <style name="SunsetTimeWidget_float_8" parent="SunsetTimeWidget_float">
        <item name="android:layout_gravity">bottom|center_horizontal</item>
    </style>
    <style name="SunsetTimeWidget_float_9" parent="SunsetTimeWidget_float">
        <item name="android:layout_gravity">bottom|right|end</item>
    </style>

    <!-- A layout for widget content within the inner frame -->
    <style name="SunsetTimeLayout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <!-- An ImageView (icon) appearing to left of text/time -->
    <style name="SunsetTimeImageView">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">@dimen/sunIcon_width</item>
        <item name="android:layout_height">@dimen/sunIcon_height</item>
        <item name="android:layout_gravity">center_vertical|right</item>
        <item name="android:layout_marginLeft">2dp</item>
        <item name="android:layout_marginRight">0dp</item>
    </style>
    <style name="SunsetTimeImageView2" parent="SunsetTimeImageView">
        <item name="android:layout_gravity">center_vertical|left</item>
    </style>

    <!-- A TextView for displaying text/time -->
    <style name="SunsetTimeTextViewBase" parent="android:TextAppearance.Small">
    </style>
    <style name="SunsetTextView" parent="SunsetTimeTextViewBase">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
