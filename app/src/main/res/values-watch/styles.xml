<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ToolbarTitleTextAppearance" parent="@style/TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">?attr/text_size_large</item>
    </style>
    <style name="ToolbarSubtitleTextAppearance" parent="@style/TextAppearance.Widget.AppCompat.Toolbar.Subtitle">
        <item name="android:textSize">?attr/text_size_xsmall</item>
    </style>

    <style name="AboutToolbarStyle">
        <item name="android:visibility">gone</item>
    </style>
    <style name="AboutTabStyle">
        <item name="android:visibility">gone</item>
    </style>

    <style name="WelcomeTextSizeGroup">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:layout_marginBottom">8dp</item>
    </style>

    <style name="DialogTitleStyle" parent="TableHeadTextView0">        <!-- rise/set header text -->
        <item name="android:textSize">?attr/text_size_medium</item>
    </style>

    <style name="EquinoxDialogTitleStyle" parent="TableHeadDateView">
        <item name="android:textSize">?attr/text_size_large</item>
        <item name="android:paddingLeft">4dp</item>
        <item name="android:paddingRight">4dp</item>
    </style>
    <style name="EquinoxDialogItemNoteStyle">
        <item name="android:textSize">?attr/text_size_small</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:antialias">true</item>
    </style>

</resources>