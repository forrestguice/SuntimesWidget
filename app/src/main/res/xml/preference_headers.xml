<?xml version="1.0" encoding="utf-8"?>
<preference-headers xmlns:tools="http://schemas.android.com/tools"
                    xmlns:android="http://schemas.android.com/apk/res/android">

    <header android:fragment="com.forrestguice.suntimeswidget.settings.fragments.GeneralPrefsFragment"
            android:icon="?attr/icActionSettings"
            android:title="@string/configLabel_general"
            android:summary=""
            tools:ignore="UnusedAttribute">

    </header>

    <header android:fragment="com.forrestguice.suntimeswidget.settings.fragments.AlarmPrefsFragment"
        android:icon="?attr/icActionAlarm"
        android:title="@string/configLabel_alarmClock"
        android:summary=""
        tools:ignore="UnusedAttribute">
    </header>

    <header android:fragment="com.forrestguice.suntimeswidget.settings.fragments.CalendarPrefsFragment"
        android:icon="?attr/icActionCalendar"
        android:title="@string/configLabel_calendars"
        android:summary=""
        tools:ignore="UnusedAttribute">
    </header>

    <header android:fragment="com.forrestguice.suntimeswidget.settings.fragments.LocalePrefsFragment"
            android:icon="?attr/icActionLocale"
            android:title="@string/configLabel_locale"
            android:summary=""
            tools:ignore="UnusedAttribute">
    </header>

    <header android:fragment="com.forrestguice.suntimeswidget.settings.fragments.PlacesPrefsFragment"
            android:icon="?attr/icActionPlace"
            android:title="@string/configLabel_places"
            android:summary=""
            tools:ignore="UnusedAttribute">
    </header>

    <header android:fragment="com.forrestguice.suntimeswidget.settings.fragments.UIPrefsFragment"
            android:icon="?attr/icActionAppearance"
            android:title="@string/configLabel_ui"
            android:summary=""
            tools:ignore="UnusedAttribute">
    </header>

    <header android:id="@+id/prefHeaderWidgets"
            android:icon="?attr/icActionWidgets"
            android:title="@string/configLabel_widgetList"
            android:summary="">

        <intent android:targetPackage="@string/packageName"
                android:targetClass="com.forrestguice.suntimeswidget.widgets.SuntimesWidgetListActivity" />

    </header>

</preference-headers>